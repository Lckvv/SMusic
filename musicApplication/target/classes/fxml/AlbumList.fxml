<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="AlbumField" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.music.controllers.AlbumListController">
   <children>
      <TextField fx:id="albumField" layoutX="151.0" layoutY="92.0" />
      <Label layoutX="47.0" layoutY="96.0" text="Albums Name" />
      <Label layoutX="51.0" layoutY="42.0" text="Bands Name" />
      <TableView fx:id="albumTableView" layoutX="1.0" layoutY="158.0" prefHeight="242.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="albumColumn" minWidth="0.0" prefWidth="599.0" text="Albums" />
        </columns>
      </TableView>
      <Button fx:id="addButton" layoutX="380.0" layoutY="71.0" mnemonicParsing="false" onAction="#addAlbumOnAction" prefHeight="25.0" prefWidth="95.0" text="Add" />
      <ComboBox fx:id="bandComboBox" layoutX="151.0" layoutY="37.0" onAction="#bandOnAction" prefWidth="150.0" />
   </children>
</AnchorPane>
