<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.HBox?>

<HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.music.controllers.TopMenuButtonsController">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <ToggleButton mnemonicParsing="false" onAction="#openListMusics" prefHeight="80.0" prefWidth="150.0" text="List Musics">
         <toggleGroup>
            <ToggleGroup fx:id="toggleButtons" />
         </toggleGroup>
      </ToggleButton>
      <Button mnemonicParsing="false" onAction="#addMusics" prefHeight="80.0" prefWidth="150.0" text="Add Musics" />
      <Button mnemonicParsing="false" onAction="#openListSearch" prefHeight="80.0" prefWidth="150.0" text="List Search" />
      <Button mnemonicParsing="false" onAction="#openListLogs" prefHeight="80.0" prefWidth="150.0" text="List Logs" />
   </children>
</HBox>
